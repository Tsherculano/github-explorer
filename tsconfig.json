{
  "compilerOptions": {                                           
    "lib": ["DOM", "DOM.Iterable", "ESNext"],                                        
    "jsx": "react-jsx",                                                             
    "moduleResolution": "node",                                                                                 
    "allowJs": true,                                                            
    "noEmit": true,                                                      
    "isolatedModules": true,                                      
    "esModuleInterop": true,                                                  
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                                           
    "skipLibCheck": true                                 
  },
 "include": ["src"]
}
